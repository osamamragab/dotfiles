def_key "j"
	scroll_down
def_key "k"
	scroll_up

def_key "J"
	page_down
def_key "K"
	page_up

def_key "h"
	previous_column
def_key "l"
	next_column

def_key "n"
	next_found_item
def_key "N"
	previous_found_item

def_key "G"
	move_end
def_key "g"
	move_home

def_key "."
	seek_forward
def_key ","
	seek_backward

def_key "="
	volume_up
def_key "+"
	show_clock

def_key "l"
	enter_directory
def_key "l"
	run_action
def_key "l"
	play_item

def_key "U"
	update_database

def_key "P"
	show_playlist

def_key "s"
	toggle_single
