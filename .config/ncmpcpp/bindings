def_key "j"
	scroll_down
def_key "k"
	scroll_up

def_key "J"
	move_selected_items_down
def_key "K"
	move_selected_items_up

def_key "h"
	previous_column
def_key "l"
	next_column

def_key "n"
	next_found_item
def_key "N"
	previous_found_item

def_key "G"
	move_end
def_key "g"
	move_home

def_key "t"
	jump_to_position_in_song

def_key "b"
	jump_to_browser

def_key "."
	seek_forward
def_key ","
	seek_backward

def_key "="
	volume_up
def_key "+"
	show_clock

def_key "-"
	volume_down
def_key "_"
	show_lyrics

def_key "l"
	enter_directory
def_key "l"
	run_action
def_key "l"
	play_item

def_key "L"
	show_lyrics
def_key "H"
	toggle_lyrics_fetcher

def_key "U"
	update_database

def_key "P"
	show_playlist

def_key "s"
	toggle_single

def_key "f"
	seek_forward
def_key "d"
	seek_backward
