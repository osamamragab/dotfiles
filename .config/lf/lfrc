set shell "sh"
set shellopts "-eu"
set ifs "\n"
set icons
set period 1
set scrolloff 10
set cursorpreviewfmt "\033[7;2m"
set autoquit true
set cleaner "~/.config/lf/cleaner"
set previewer "~/.config/lf/previewer"

map <enter> shell
map <esc> !true

map x $$f
map X !$f

cmd open &open $f
map o open $f

cmd extract $extract $f
